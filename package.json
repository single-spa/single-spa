{
  "name": "single-spa",
  "version": "7.0.0-beta.5",
  "description": "The router for easy microfrontends",
  "type": "module",
  "main": "./lib/esm/single-spa.min.js",
  "types": "./lib/esm/single-spa.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "development": {
        "import": {
          "types": "./lib/esm/single-spa.d.ts",
          "default": "./lib/esm/single-spa.min.js"
        },
        "require": {
          "types": "./lib/umd/single-spa.d.cts",
          "default": "./lib/umd/single-spa.min.cjs"
        }
      },
      "production": {
        "import": {
          "types": "./lib/esm/single-spa.d.ts",
          "default": "./lib/esm/single-spa.min.js"
        },
        "require": {
          "types": "./lib/umd/single-spa.d.cts",
          "default": "./lib/umd/single-spa.min.cjs"
        }
      },
      "default": {
        "import": {
          "types": "./lib/esm/single-spa.d.ts",
          "default": "./lib/esm/single-spa.min.js"
        },
        "require": {
          "types": "./lib/umd/single-spa.d.cts",
          "default": "./lib/umd/single-spa.min.cjs"
        }
      }
    },
    "./package.json": "./package.json",
    "./lib/*": "./lib/*"
  },
  "scripts": {
    "build": "pnpm run clean && concurrently pnpm:build:dev pnpm:build:prod && rimraf ./lib/umd/types-unbundled ./lib/esm/types-unbundled",
    "build:prod": "rollup -c --environment ROLLUP_ENV:'production'",
    "build:dev": "rollup -c",
    "build:analyze": "rollup -c --environment ANALYZER:'analyzer'",
    "watch": "rollup -c -w",
    "clean": "rimraf lib",
    "test": "concurrently -n w: 'pnpm:test:*'",
    "test:browser": "cross-env BABEL_ENV=test jest --config jest-browser.config.js",
    "test:node": "cross-env BABEL_ENV=test jest --config jest-node.config.js",
    "test:lockfile": "js-correct-lockfile pnpm",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "eslint src",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "lib",
    "typings"
  ],
  "homepage": "https://single-spa.js.org",
  "repository": "https://github.com/single-spa/single-spa",
  "bugs": "https://github.com/single-spa/single-spa/issues",
  "keywords": [
    "microfrontends",
    "single",
    "page",
    "application",
    "framework",
    "react",
    "angular",
    "vue",
    "router",
    "microservices",
    "frontend"
  ],
  "author": "Joel Denning",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.9",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/parser": "^8.26.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.2",
    "custom-event": "^1.0.1",
    "eslint": "9.21.0",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-plugin-es5": "^1.5.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-correct-lockfile": "^1.0.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.9",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.8.2"
  },
  "browserslist": [
    ">5%",
    "not ie <= 11"
  ]
}
